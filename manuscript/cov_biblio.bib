@article{Penttila2021,
abstract = {The COVID-19 pandemic poses a major burden on healthcare and economic systems across the globe. Even though a majority of the population develops only minor symptoms upon SARS-CoV-2 infection, a significant number are hospitalized at intensive care units (ICU) requiring critical care. While insights into the early stages of the disease are rapidly expanding, the dynamic immunological processes occurring in critically ill patients throughout their recovery at ICU are far less understood. Here, we have analysed whole blood samples serially collected from 40 surviving COVID-19 patients throughout their recovery in ICU using high-dimensional cytometry by time-of-flight (CyTOF) and cytokine multiplexing. Based on the neutrophil-to-lymphocyte ratio (NLR), we defined four sequential immunotypes during recovery that correlated to various clinical parameters, including the level of respiratory support at concomitant sampling times. We identified classical monocytes as the first immune cell type to recover by restoration of HLA-DR-positivity and the reduction of immunosuppressive CD163 + monocytes, followed by the recovery of CD8 + and CD4 + T cell and non-classical monocyte populations. The identified immunotypes also correlated to aberrant cytokine and acute-phase reactant levels. Finally, integrative analysis of cytokines and immune cell profiles showed a shift from an initially dysregulated immune response to a more coordinated immunogenic interplay, highlighting the importance of longitudinal sampling to understand the pathophysiology underlying recovery from severe COVID-19.},
author = {Penttil{\"{a}}, P. A. and {Van Gassen}, S. and Panovska, D. and Vanderbeke, L. and {Van Herck}, Y. and Quintelier, K. and Emmaneel, A. and Filtjens, J. and Malengier-Devlies, B. and Ahmadzadeh, K. and {Van Mol}, P. and Borr{\`{a}}s, D. M. and Antoranz, A. and Bosisio, F. M. and Wauters, E. and Martinod, K. and Matthys, P. and Saeys, Y. and Garg, A. D. and Wauters, J. and {De Smet}, F. and Casaer, Michael and Dauwe, Dieter and Gunst, Jan and Hermans, Greet and Humblet-Baron, Stephanie and Lambrechts, Diether and Liston, Adrian and Lorent, Natalie and Meersseman, Philippe and Neyts, Johan and Proost, Paul and Raes, Jeroen and Rex, Stephen and Tejpar, Sabine and Thevissen, K. and Tousseyn, Thomas and Weynand, Birgit and Wilmer, Alexander and Wouters, Carine},
