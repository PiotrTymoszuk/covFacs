issn = {14690691},
keywords = {Copper,HIV,Immunity,Iron,Malaria,Manganese,Microbe,Selenium,Sepsis,Zinc},
month = {jan},
number = {1},
pages = {16--23},
publisher = {Elsevier B.V.},
title = {{Role of divalent metals in infectious disease susceptibility and outcome}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28143784},
volume = {24},
year = {2018}
}
