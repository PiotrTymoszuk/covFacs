journal = {Frontiers in pharmacology},
keywords = {anti-ferroportin monoclonal antibody,antibody engineering,ferroportin,ferroportin antagonist,fluorescently-labeled hepcidin,hepcidin,iron metabolism,receptor internalization},
pages = {838},
